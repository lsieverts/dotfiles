rofi.run-command: fish -c '{cmd}'
rofi.run-list-command: 'fish -c functions'
rofi.color-window: argb:c82d303b, #7c8389, #1d1f21
!rofi.color-normal: argb:31d1f21, #c4cbd4, argb:96404552, #4084d6, #f9f9f9
