Xft.dpi:       96
Xft.antialias: 1
Xft.hinting:   1
Xft.rgba:      rgb
Xft.autohint:  0
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault

! Dracula Xresources palette
!*.foreground: #F8F8F2
!*.background: #282A36
!*.color0:     #000000
!*.color8:     #4D4D4D
!*.color1:     #FF5555
!*.color9:     #FF6E67
!*.color2:     #50FA7B
!*.color10:    #5AF78E
!*.color3:     #F1FA8C
!*.color11:    #F4F99D
!*.color4:     #BD93F9
!*.color12:    #CAA9FA
!*.color5:     #FF79C6
!*.color13:    #FF92D0
!*.color6:     #8BE9FD
!*.color14:    #9AEDFE
!*.color7:     #BFBFBF
!*.color15:    #E6E6E6

! Solarized dark
!.*background:            #1c1c1c
!.*foreground:            #626262
!.*fading:                40
!.*fadeColor:             #1c1c1c
!.*cursorColor:           #8a8a8a
!.*pointerColorBackground:#585858
!.*pointerColorForeground:#8a8a8a
!
!!! black dark/light
!.*color0:                #262626
!.*color8:                #1c1c1c
!
!!! red dark/light
!.*color1:                #d70000
!.*color9:                #d75f00
!
!!! green dark/light
!.*color2:                #5f8700
!.*color10:               #585858
!
!!! yellow dark/light
!.*color3:                #af8700
!.*color11:               #626262
!
!!! blue dark/light
!.*color4:                #0087ff
!.*color12:               #808080
!
!!! magenta dark/light
!.*color5:                #af005f
!.*color13:               #5f5faf
!
!!! cyan dark/light
!.*color6:                #00afaf
!.*color14:               #8a8a8a
!
!!! white dark/light
!.*color7:                #e4e4e4
!.*color15:               #ffffd7

!*.font:	Liberation Mono:pixelsize=14:antialias=true:autohint=true;
!*.font:	Inconsolata:pixelsize=16:antialias=true:autohint=true;
!*.font:	FiraCode:pixelsize=16:antialias=true:autohint=true;
!*.font:     xft:bitstream vera sans mono:size=16:antialias=true
!*.boldFont: xft:bitstream vera sans mono:bold:size=16:antialias=true
!*.alpha: 200

!rofi.font:	Inconsolata:pixelsize=16:antialias=true:autohint=true;
rofi.run-command: fish -c '{cmd}'
rofi.run-list-command: 'fish -c functions'
rofi.font: Inconsolata 14


