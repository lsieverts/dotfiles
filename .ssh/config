Host venus
    User patrick
    HostName venus.geogr.uni-jena.de
    IdentityFile ~/.ssh/id_rsa

Host aur aur.archlinux.org
    User aur
    Hostname aur.archlinux.org
    IdentityFile ~/.ssh/keys/aur

Host edi
    User patrick
    Hostname edi.geogr.uni-jena.de
    IdentityFile ~/.ssh/id_rsa
    Ciphers aes256-ctr
    #Compression no
    #RemoteCommand cd /; $SHELL -il

Host jupiter
    User patrick
    Hostname jupiter.geogr.uni-jena.de
    IdentityFile ~/.ssh/id_rsa

Host mars
    User patrick
    Hostname mars.geogr.uni-jena.de
    IdentityFile ~/.ssh/id_rsa

Host github
    User git
    Hostname github.com
    IdentityFile ~/.ssh/id_rsa

Host *
  IgnoreUnknown UseKeychain
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
